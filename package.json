{
  "name": "@swiftuijs/ui",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*/index.js",
      "types": "./dist/components/*/index.d.ts"
    },
    "./common/*": {
      "import": "./dist/common/*.js",
      "types": "./dist/common/*.d.ts"
    },
    "./contexts/*": {
      "import": "./dist/contexts/*.js",
      "types": "./dist/contexts/*.d.ts"
    },
    "./hooks/*": {
      "import": "./dist/hooks/*.js",
      "types": "./dist/hooks/*.d.ts"
    },
    "./style/*": {
      "import": "./dist/style/*.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "prebuild-sb": "rimraf docs",
    "sb": "storybook dev -p 6006",
    "build-sb": "storybook build -o docs",
    "postbuild-sb": "node scripts/post-build-sb.js",
    "build": "rimraf dist && vite build && node scripts/copy-types.js && tsc -p tsconfig.build.json && node scripts/fix-dts-paths.js",
    "gen": "cross-env NODE_OPTIONS='--import tsx' plop --plopfile=scripts/gen-component.ts",
    "lint": "eslint . --max-warnings 0",
    "build-all": "pnpm build && pnpm build-sb",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.0.0",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-links": "10.1.10",
    "@storybook/addon-onboarding": "10.1.10",
    "@storybook/react": "10.1.10",
    "@storybook/react-vite": "10.1.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/dom-view-transitions": "^1.0.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "cross-env": "^7.0.3",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-storybook": "^10.1.10",
    "jsdom": "^27.3.0",
    "plop": "^4.0.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "remark-gfm": "^4.0.1",
    "rimraf": "^6.0.1",
    "sass": "^1.83.0",
    "storybook": "^10.1.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.0",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "packageManager": "pnpm@10.0.0",
  "source": "src/index.tsx",
  "repository": "https://github.com/swiftuijs/ui.git",
  "author": "Saiya",
  "license": "MIT",
  "dependencies": {
    "plain-store": "^0.2.2"
  }
}
